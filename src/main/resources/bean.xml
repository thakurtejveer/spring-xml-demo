<beans xmlns = "http://www.springframework.org/schema/beans"
       xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">


    <bean id="actor" class="com.stackroute.domain.Actor">
        <property name="name" value="Diljit Dosanjh"/>
        <property name="age" value="35"/>
        <property name="gender" value="male"/>
    </bean>
    <bean id="movie" class="com.stackroute.domain.Movie">
        <constructor-arg name="actor" ref="actor"/>
    </bean>
    <bean id="beanLifeCycle" class="com.stackroute.domain.BeanLifecycleDemoBean" init-method="customInit" destroy-method="customDestroy"></bean>
    <bean id="beanPostProcessor" class="com.stackroute.domain.BeanPostProcessorDemoBean"/>
<!--    <bean id="movieX" class="com.stackroute.domain.Movie" autowire="byType"></bean>-->

</beans>